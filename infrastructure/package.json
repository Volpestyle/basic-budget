{
  "name": "basic-budget-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for Basic Budget application",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "bin": "bin"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "deploy:dev": "cdk deploy --all --context environment=dev",
    "deploy:staging": "cdk deploy --all --context environment=staging",
    "deploy:prod": "cdk deploy --all --context environment=prod",
    "destroy": "cdk destroy --all",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap"
  },
  "keywords": [
    "aws",
    "cdk",
    "infrastructure",
    "budget",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.5.1",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "aws-cdk-lib": "2.100.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}